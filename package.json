{
    "name": "rollup-plugin-typedoc",
    "author": "Sergey Chernykh <s-chernykh@outlook.com> (https://github.com/serglider/)",
    "homepage": "https://github.com/serglider/shake-detector",
    "version": "0.1.0",
    "description": "Runs typedoc compilation as a rollup plugin",
    "keywords": [
        "typedoc",
        "rollup",
        "rollup-plugin",
        "documentation",
        "typescript"
    ],
    "license": "MIT",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/serglider/shake-detector.git"
    },
    "dependencies": {
        "lodash.clone": "^4.5.0",
        "lodash.merge": "^4.6.0"
    },
    "peerDependencies": {
        "rollup": "2.x",
        "typedoc": ">= 0.5.0"
    },
    "prettier": {
        "trailingComma": "es5",
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "arrowParens": "avoid"
    }
}
